Task:3

class person{
    constructor(name= '', age = 0, qualification = {}, nationality = ''  ){
        this.name = name;
        this.age = age;
        this.qualification = qualification;
        this.nationality = nationality;
    }
}

function getQualification(person = [], qualification = []){
    return person.filter((q)=> q.qualification === qualification);
}

const vijay = new person("vijay", 25, "physics", "india")
const vijay1 = new person("vijay1", 26,  "biology", "america")
const vijay2= new person("vijay3", 27, "zoology", "ukanda")
const vijay3 = new person("vijay4", 28, "maths", "singapore")

var personArray = [vijay, vijay1, vijay2, vijay3]

console.log(getQualification(personArray,"physics"));
-------------------------------------------------------------------------------------------------
