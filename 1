task1:
    class movie{
        constructor(title = '', studio = '', rating = 'pg'){
            this.title = title;
            this.studio = studio;
            this.rating = rating;
        }
    }

    function getPG(movie = [], rating =''){
    return movie.filter((m) => m.rating===rating);
    }

    const CasinoRoyale = new movie("Casino Royale", "Eon Productions",'PG13');
    const rockStar = new movie("rockstar", "padma studio", 'PG');
    const vikram = new movie("vikram", "kamala studio", 'PG');
    const arjun = new movie("arjun", "kamala studio", 'R');
    const vijay = new movie("vijay", "kamala studio", 'R');

    const movieArray = [CasinoRoyale, rockStar, vikram, arjun, vijay];

    console.log(getPG(movieArray, 'R'));
